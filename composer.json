{
    "name": "macellan/laravel-js-routes",
    "description": "Laravel routes from Javascript",
    "license": "MIT",
    "keywords": ["laravel", "generators", "artisan", "routing"],
    "authors": [
        {
            "name": "Fatih Aytekin",
            "email": "fatih@aytekin.me"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "~5.5 || ~6.0 || ~7.0 || ~8.0",
        "illuminate/console": "~5.5 || ~6.0 || ~7.0|| ~8.0",
        "illuminate/filesystem": "~5.5 || ~6.0 || ~7.0|| ~8.0",
        "illuminate/routing": "~5.5 || ~6.0 || ~7.0|| ~8.0"
    },
    "require-dev": {
        "illuminate/events": "~5.5 || ~6.0 || ~7.0|| ~8.0",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Macellan\\LaravelJsRoutes\\": "src/"
        },
        "classmap": [
            "src/Commands",
            "src/Generators"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Macellan\\LaravelJsRoutes\\Test\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Macellan\\LaravelJsRoutes\\LaravelJsRoutesServiceProvider"
            ]
        }
    }
}
